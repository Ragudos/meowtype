{
    "compilerOptions": {
        "rootDir": "src",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2023"
        ],
        "module": "ESNext",
        "target": "ES2023",
        "moduleResolution": "Bundler",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "allowUnreachableCode": false,
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ]
        },
        "noEmit": true
    },
    "include": [
        "src",
        "global.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}